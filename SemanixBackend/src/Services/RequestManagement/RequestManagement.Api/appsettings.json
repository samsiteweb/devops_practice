{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security.Bearer": {
    "Authority": "https://localhost:{sts_port}",
    "Audience": "api"
  },
  "DaprSidekick": {
    "Sidecar": {
      "AppId": "request-management",
      "ComponentsDirectory": "../dapr/components",
      "ConfigFile": "../dapr/config.yaml"
    }
  },
  "Bugsnag": {
    "ApiKey": "",
    "AppVersion": "1.0.0",
    "ReleaseStage": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Category:0} - {Message}{NewLine:1}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "Dapr": {
    "Enabled": false
  }
}